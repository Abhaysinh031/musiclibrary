{
    "version": 2,
    "builds": [
      {
        "src": "install_requirements.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb"
        }
      },
      {
        "src": "manage.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/manage.py"
      }
    ]
  }
  